package pattern;
import java.util.Scanner;
public class pattern {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input=new Scanner(System.in);
		System.out.println("Enter the character and max number of time printed:");
		char c=input.next().charAt(0); 
		int n=input.nextInt(); 
		for(int i=1;i<=n;i++) 
		{ 
		for(int j=1;j<=i;j++) 
		{ 
		System.out.print(c); 
		} 
		System.out.println();
	}
}
}
